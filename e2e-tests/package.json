    "typescript": "^5.6.3",
    "vite": "^5.4.8"
  },
  "dependencies": {
    "@dashify/auth": "^0.0.2",
    "@dashify/chakra": "^0.0.3",
    "@dashify/core": "^0.0.2",
    "@dashify/provider": "^0.0.1",
    "@tanstack/react-query": "^5.59.11",
    "axios": "^1.7.7",
    "concurrently": "^9.0.1",
    "cypress": "^13.15.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.27.0",
